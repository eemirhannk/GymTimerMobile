{
  "expo": {
    "name": "Gym Timer",
    "slug": "gym-timer-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "description": "Spor salonunda kullanabileceğiniz profesyonel zamanlayıcı uygulaması",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#F3F4F6"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gymtimer.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#F3F4F6"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.gymtimer.mobile"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
